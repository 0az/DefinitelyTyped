{
    "extends": "dtslint/dt.json",
    "rules": {
        "interface-name": false,
        "no-empty-interface": false,
        "no-redundant-jsdoc": false,
        "no-unnecessary-class": false,
        "npm-naming": [
            true,
            {
                "mode": "name-only"
            }
        ]
    }
}