{
    "extends": "dtslint/dt.json",
    "rules": {
        "callable-types": false,
        "max-line-length": false,
        "no-redundant-jsdoc": false,
        "no-var-keyword": false,
        "npm-naming": [
            true,
            {
                "mode": "name-only"
            }
        ],
        "prefer-method-signature": false,
        "semicolon": false,
        "unified-signatures": false
    }
}