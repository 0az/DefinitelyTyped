{
    "extends": "dtslint/dt.json",
    "rules": {
        "ban-types": false,
        "callable-types": false,
        "eofline": false,
        "no-consecutive-blank-lines": false,
        "no-padding": false,
        "no-redundant-jsdoc": false,
        "no-var-keyword": false,
        "npm-naming": [
            true,
            {
                "mode": "name-only"
            }
        ],
        "prefer-const": false,
        "prefer-method-signature": false,
        "trim-file": false,
        "typedef-whitespace": false,
        "whitespace": false
    }
}